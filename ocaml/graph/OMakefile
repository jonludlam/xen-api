OCAML_LIBS    = ../util/xapi_version ../idl/xapi-consts ../xapi-types/common ../xapi-client/client ../idl/xapi-datamodel
OCAMLINCLUDES = ../idl/ocaml_backend ../idl ../xapi-types ../xapi-client
OCAMLPACKS    = xml-light2 stdext stunnel http-svr xcp rpclib uuid

OCamlProgram(graph, graph)

.PHONY: clean
clean:
	rm -f $(CLEAN_OBJS) graph

.PHONY: install
install:
	mkdir -p $(DESTDIR)$(OPTDIR)/debug
	$(IPROG) graph $(DESTDIR)$(OPTDIR)/debug


